    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@webpack-cli/serve": "^1.6.0",
    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.5.1",
    "conventional-changelog": "^3.1.24",
