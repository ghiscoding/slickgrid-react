{
  "name": "slickgrid-react",
  "version": "1.0.0-beta.1",
  "description": "Slickgrid components made available in React",
  "keywords": [
    "react",
    "datagrid",
    "datatable",
    "plugin",
    "slickgrid"
  ],
  "homepage": "https://github.com/ghiscoding/slickgrid-react",
  "bugs": {
    "url": "https://github.com/ghiscoding/slickgrid-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghiscoding/slickgrid-react.git"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "contributors": [
    {
      "name": "Ghislain B."
    },
    {
      "name": "Devin Garner"
    },
    {
      "name": "Javier Soques"
    },
    {
      "name": "Dustin Garner"
    }
  ],
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "scripts": {
    "delete:dist": "cross-env rimraf dist",
    "lint": "eslint src/slickgrid-react/** --ext .ts --quiet",
    "build:amd": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/amd --module amd",
    "postbuild:amd": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/amd",
    "build:commonjs": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/commonjs",
    "build:esm": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/esm --module esnext --target es2018",
    "postbuild:esm": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/esm",
    "build:native-modules": "cross-env tsc --project src/slickgrid-react/tsconfig.build.json --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "cross-env copyfiles --up 2 src/slickgrid-react/**/*.html dist/native-modules",
    "//prebuild": "npm-run-all delete:dist lint",
    "prebuild": "npm-run-all delete:dist",
    "build": "npm-run-all build:amd build:commonjs build:esm build:native-modules",
    "postbuild": "npm-run-all copy-i18n:dist copy-asset-lib",
    "build:with-e2e": "npm-run-all build cypress:ci",
    "copy-asset-lib": "cross-env copyfiles --up 2 src/assets/lib/** dist",
    "copy-i18n:dist": "cross-env copyfiles --up 3 src/assets/i18n/**/*.* dist/i18n",
    "predoc": "cross-env rimraf api.json && rimraf dist/doc-temp && tsc --project src/slickgrid-react/tsconfig.build.json --outFile dist/doc-temp/react-validation.js && node shape-defs && copyfiles tsconfig.json dist/doc-temp",
    "doc": "cross-env typedoc --json api.json --excludeExternals --includeDeclarations --mode modules --target ES6 --name react-validation-docs dist/doc-temp/",
    "postdoc": "cross-env node shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p react -i CHANGELOG.md -s",
    "bump-version": "npm --no-git-tag-version version",
    "prepare-release": "cross-env npm run changelog && npm run build && npm run doc",
    "release": "standard-version",
    "start:dev": "webpack serve --env development",
    "cypress": "cd test/cypress && node node_modules/cypress/bin/cypress open",
    "cypress:ci": "cd test/cypress && npm run cypress:ci",
    "test:ci": "npx jest --runInBand --coverage=true --ci --config ./test/jest.config.js",
    "test:jest": "npx jest --watch --detectOpenHandles --config test/jest.config.js",
    "test:jest:coverage": "npx jest --detectOpenHandles --runInBand --coverage --config test/jest.config.js",
    "build:demo": "webpack --env production",
    "update-npm-versions": "npm-check --skip-unused --update-all",
    "//debug": "npm run build && copyfiles dist/**/* c:/temp/slickgrid-react-test/node_modules/slickgrid-react"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@slickgrid-universal/common": "^1.1.0",
    "@slickgrid-universal/custom-footer-component": "^1.1.1",
    "@slickgrid-universal/empty-warning-component": "^1.1.1",
    "@slickgrid-universal/event-pub-sub": "^1.1.1",
    "@slickgrid-universal/pagination-component": "^1.1.1",
    "@slickgrid-universal/rxjs-observable": "^1.1.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.5.0",
    "babel-loader": "^8.2.3",
    "dequal": "^2.0.2",
    "dompurify": "^2.3.4",
    "font-awesome": "^4.7.0",
    "i18next": ">= 14.1.1",
